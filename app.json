{
  "expo": {
    "name": "OpenSoop",
    "slug": "UNI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ApplogoIcon.png",
    "resizeMode": "contain",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ApplogoIcon.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.leejt.UNI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b9f12324-ead1-4524-bbac-d70a0932dbfc"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ]
    ]
  }
}